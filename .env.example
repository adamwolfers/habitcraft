# Habit Tracker - Environment Variables Example
# Copy this file to .env and update with your values

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=habittracker
DB_USER=habituser
DB_PASSWORD=habitpass
DATABASE_URL=postgresql://habituser:habitpass@localhost:5432/habittracker

# Authentication
JWT_SECRET=your-secret-key-change-in-production-use-long-random-string
JWT_EXPIRES_IN=7d

# Application
NODE_ENV=development
ENVIRONMENT=development
PORT=3000

# API URLs (for frontends)
NEXT_PUBLIC_API_URL=http://localhost:3000/api/v1
REACT_APP_API_URL=http://localhost:3000/api/v1
VITE_API_URL=http://localhost:3000/api/v1
